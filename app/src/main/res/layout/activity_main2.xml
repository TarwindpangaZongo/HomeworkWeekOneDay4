<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">

    <TextView
    android:id="@+id/tvFirstNameDisplay"
    android:layout_width="380dp"
    android:layout_height="35dp"
    android:layout_marginStart="16dp"
    android:textSize="25sp"
    android:textColor="#0A13E4"
    android:textStyle="bold"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintHorizontal_bias="0.15"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.1" />


    <TextView
    android:id="@+id/tvLastNameDisplay"
    android:layout_width="380dp"
    android:layout_height="35dp"
    android:layout_marginStart="16dp"
    android:textSize="25sp"
    android:textColor="#0A13E4"
        android:textStyle="bold"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintHorizontal_bias="0.15"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.15" />

    <TextView
    android:id="@+id/tvAddressDisplay"
    android:layout_width="380dp"
    android:layout_height="35dp"
    android:layout_marginStart="16dp"
    android:textSize="25sp"
    android:textColor="#19E40A"
    android:textStyle="italic"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintHorizontal_bias="0.15"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.2" />

    <TextView
    android:id="@+id/tvCityDisplay"
    android:layout_width="380dp"
    android:layout_height="35dp"
    android:layout_marginStart="16dp"
    android:textSize="25sp"
    android:textColor="#E4560A"
    android:textStyle="italic"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintHorizontal_bias="0.15"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.25" />

    <TextView
    android:id="@+id/tvStateDisplay"
    android:layout_width="380dp"
    android:layout_height="35dp"
    android:layout_marginStart="16dp"
    android:textSize="25sp"
    android:textColor="#E40A3C"
    android:textStyle="italic"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintHorizontal_bias="0.15"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.3" />

    <TextView
    android:id="@+id/tvPostalCodeDisplay"
    android:layout_width="380dp"
    android:layout_height="35dp"
    android:layout_marginStart="16dp"
    android:textSize="25sp"
    android:textColor="#E40A1E"
    android:textStyle="italic"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintHorizontal_bias="0.15"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.35" />



</android.support.constraint.ConstraintLayout>